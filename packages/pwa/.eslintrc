{
  "root": true,
  "extends": [
    "plugin:@next/next/recommended",
    "@flextremedev/eslint-config-react-typescript"
  ],
  "rules": {
    "@typescript-eslint/explicit-function-return-type": "warn"
  },
  "overrides": [
    {
      "files": ["src/pages/**/*.{ts,tsx}"],
      "rules": {
        "import/no-default-export": "off"
      }
    }
  ]
}
